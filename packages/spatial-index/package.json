{
  "name": "spatial-index-js",
  "version": "1.0.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "repository": "https://github.com/wkgreat/spatial-index.git",
  "author": "ke.wang <wkgreat@outlook.com>",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjd/index.js"
    },
    "./rtree": {
      "import": "./dist/esm/rtree/rtree.js",
      "require": "./dist/cjs/rtree/rtree.js"
    },
    "./rtree_render": {
      "import": "./dist/esm/rtree/rtree_render.js",
      "require": "./dist/cjs/rtree/rtree_render.js"
    },
    "./utils": {
      "import": "./dist/esm/utils/utils.js",
      "require": "./dist/cjs/utils/utils.js"
    }
  },
  "devDependencies": {
    "@types/jsts": "^0.17.24",
    "jsts": "^2.12.1"
  },
  "scripts": {
    "build:types": "npx tsc --emitDeclarationOnly",
    "build": "yarn run build:types && rollup -c",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:strict": "eslint src --max-warnings=0",
    "test": "jest --runInBand --no-watchman",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "cytoscape": "^3.33.1"
  }
}
